import JoinTeam from '../components/JoinTeam.vue';
import JoinTheTeamBlock from '../components/JoinTheTeamBlock.astro';
import JoinTheTeamText from '../components/JoinTheTeamText.astro';
import Layout from '../layouts/Layout.astro';

const { data: { text } } = await loadContent('join_the_team')
